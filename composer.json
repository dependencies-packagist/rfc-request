{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "rfc/request",
    "description": "Note that this is not a request of its own. It is merely an interface that describes a request.",
    "keywords": [
        "rfc",
        "request"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dependencies-packagist/rfc-request",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Rfc\\Request\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
